{
  "name": "ffhq-convert-json",
  "version": "0.1.0",
  "description": "Converts the JSON metadata contained in the FFHQ dataset to model-compatible formats",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib && tsc --build src",
    "exec": "npm run build && node lib/index.js",
    "localInstall": "npm run build && npm i -g ."
  },
  "bin": {
    "ffhq-convert-json": "bin/ffhq-convert-json.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trinitrotoluene/ffhq-convert-json.git"
  },
  "author": "trinitrotoluene",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/trinitrotoluene/ffhq-convert-json/issues"
  },
  "homepage": "https://github.com/trinitrotoluene/ffhq-convert-json#readme",
  "dependencies": {
    "commander": "^9.1.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "typescript": "^4.6.2"
  }
}
